	$(AM_V_at)$(am__mv) $(DEPDIR)/mga_crtc2_vid_la-mga_vid.Tpo $(DEPDIR)/mga_crtc2_vid_la-mga_vid.Plo
#	$(AM_V_CC)source='mga_vid.c' object='mga_crtc2_vid_la-mga_vid.lo' libtool=yes \
#	DEPDIR=$(DEPDIR) $(CCDEPMODE) $(depcomp) \
