install-vidixLTLIBRARIES: $(vidix_LTLIBRARIES)
	@$(NORMAL_INSTALL)
	@list='$(vidix_LTLIBRARIES)'; test -n "$(vidixdir)" || list=; \
	list2=; for p in $$list; do \
	  if test -f $$p; then \
	    list2="$$list2 $$p"; \
	  else :; fi; \
	done; \
	test -z "$$list2" || { \
	  echo " $(MKDIR_P) '$(DESTDIR)$(vidixdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(vidixdir)" || exit 1; \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 '$(DESTDIR)$(vidixdir)'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 "$(DESTDIR)$(vidixdir)"; \
	}

uninstall-vidixLTLIBRARIES:
	@$(NORMAL_UNINSTALL)
	@list='$(vidix_LTLIBRARIES)'; test -n "$(vidixdir)" || list=; \
	for p in $$list; do \
	  $(am__strip_dir) \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f '$(DESTDIR)$(vidixdir)/$$f'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f "$(DESTDIR)$(vidixdir)/$$f"; \
	done

clean-vidixLTLIBRARIES:
	-test -z "$(vidix_LTLIBRARIES)" || rm -f $(vidix_LTLIBRARIES)
	@list='$(vidix_LTLIBRARIES)'; for p in $$list; do \
	  dir="`echo $$p | sed -e 's|/[^/]*$$||'`"; \
	  test "$$dir" != "$$p" || dir=.; \
	  echo "rm -f \"$${dir}/so_locations\""; \
	  rm -f "$${dir}/so_locations"; \
	done
